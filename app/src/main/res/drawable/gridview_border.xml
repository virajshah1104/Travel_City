<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item >
        <shape android:shape="rectangle">

            <solid android:color="#d3d3da" />
            <corners
                android:topLeftRadius="3.5dp"
                android:topRightRadius="3.5dp"
                android:bottomLeftRadius="3.5dp"
                android:bottomRightRadius="3.5dp"/>
        </shape>
    </item>
    <item android:right="2dp"  android:bottom="2dp" android:left="1dp" android:top="1dp">
        <shape
            android:shape="rectangle">
            <solid android:color="#efeff4"/>
            <corners android:radius="3dp" />
        </shape>
    </item>
</layer-list >
